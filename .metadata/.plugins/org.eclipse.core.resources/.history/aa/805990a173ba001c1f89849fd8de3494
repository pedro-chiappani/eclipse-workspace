package ar.edu.unlp.info.oo2.p2_ejercicio_1;

public class TopografiaSimple implemetns Topografia{

	private int propAgua;
	
	public double calcularProporcionAgua() {
		return this.propAgua;
	}
	
	public double calcularProporcionTierra() {
		return 1-propAgua;
	}
	
	public Boolean compararTopografia(TopografiaSimple topo) {
		return this.propAgua == topo.propAgua;
	}
}
